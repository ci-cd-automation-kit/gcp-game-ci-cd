{
    "env": {
        "es6": true,
        "jasmine": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": ["error", "tab"],
        "lines-between-class-members": "error",
        "quotes": ["error", "single"],
        "semi": "error",
        "space-before-blocks": "error"
    }
}
